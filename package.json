{
  "name": "abyssts",
  "version": "0.0.1",
  "author": "Alpha",
  "description": "Node.JS framework bootstrap",
  "license": "MIT",
  "keywords": ["abyssts", "vite", "node"],
  "type": "module",
  "private": true,
  "scripts": {
    "prepare": "husky || true",
    "preinstall": "npx only-allow pnpm",
    "clear": "pnpm -r exec rimraf node_modules dist .turbo lib && pnpm rimraf node_modules .turbo",
    "update-packages": "pnpm update -i -r --latest",
    "build": "turbo run build --filter=@abyssts/internal-vitest"
  },
  "packageManager": "pnpm@8.15.6",
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@config/tsconfig": "workspace:^",
    "@types/globals": "workspace:^",
    "@vitest/coverage-istanbul": "^1.6.0",
    "globals": "^15.6.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "rimraf": "^5.0.7",
    "tsup": "^8.1.0",
    "turbo": "^2.0.6",
    "typescript": "^5.5.2",
    "vite": "^5.3.2",
    "vitest": "^1.6.0"
  },
  "dependencies": {
    "tslib": "^2.6.3"
  }
}
