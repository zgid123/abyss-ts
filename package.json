{
  "name": "abyss.ts",
  "version": "0.0.1",
  "author": "Alpha",
  "description": "Node.JS framework bootstrap",
  "repository": {
    "type": "git",
    "url": "git@github.com:zgid123/abyss-ts.git"
  },
  "license": "MIT",
  "keywords": [
    "abyss",
    "abyssts",
    "abyssjs",
    "abyss.js",
    "abyss.ts",
    "node",
    "express"
  ],
  "type": "module",
  "private": true,
  "scripts": {
    "prepare": "husky || true",
    "preinstall": "npx only-allow pnpm",
    "clear": "pnpm -r exec rimraf node_modules dist .turbo lib && pnpm rimraf node_modules .turbo",
    "update-packages": "pnpm update -i -r --latest",
    "build": "turbo run build",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "packageManager": "pnpm@8.15.6",
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@config/tsconfig": "workspace:^",
    "@swc/core": "^1.6.13",
    "@types/globals": "workspace:^",
    "@vitest/coverage-istanbul": "^2.0.1",
    "globals": "^15.8.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "rimraf": "^6.0.0",
    "tsup": "^8.1.0",
    "turbo": "^2.0.6",
    "typescript": "^5.5.3",
    "vitepress": "^1.3.0",
    "vitest": "^2.0.1"
  },
  "dependencies": {
    "tslib": "^2.6.3"
  }
}
